{
  "name": "tech-demo",
  "private": true,
  "version": "1.0.0",
  "description": "Tech demo",
  "license": "MIT",
  "engines": {
    "node": "16.x"
  },
  "scripts": {
    "nest": "nest",
    "next": "cd src/client && next",
    "prebuild": "rimraf dist",
    "build": "NODE_ENV=production nest build && cd src/client && next build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch --preserveWatchOutput",
    "start:debug": "nest start --debug --watch --preserveWatchOutput",
    "start:prod": "NODE_ENV=production node dist/src/api/main",
    "lint": "eslint \"{src,test}/**/*.{ts,tsx}\"",
    "test": "jest --passWithNoTests --collect-coverage --setupFiles dotenv-flow/config",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json --setupFiles dotenv-flow/config",
    "console": "ts-node -r tsconfig-paths/register src/api/console.ts"
  },
  "dependencies": {
    "@golevelup/nestjs-stripe": "^0.3.2",
    "@hookform/resolvers": "^2.8.8",
    "@nestjs/common": "^8.4.4",
    "@nestjs/config": "^2.0.0",
    "@nestjs/core": "^8.4.4",
    "@nestjs/platform-express": "^8.4.4",
    "@nextui-org/react": "1.0.2-beta.4",
    "@reduxjs/toolkit": "^1.8.1",
    "@stripe/react-stripe-js": "^1.7.0",
    "@stripe/stripe-js": "^1.25.0",
    "autoprefixer": "^10.4.4",
    "axios": "^0.26.1",
    "commander": "^7.0.0",
    "express": "^4.17.3",
    "nestjs-console": "^7.0.1",
    "next": "^12.1.4",
    "postcss": "^8.4.12",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.28.1",
    "react-json-view": "^1.21.3",
    "react-redux": "^7.2.8",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.5",
    "stripe": "^8.210.0",
    "tailwindcss": "^3.0.24",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.2.5",
    "@nestjs/schematics": "^8.0.10",
    "@nestjs/testing": "^8.4.4",
    "@types/express": "^4.17.8",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.23",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "dotenv-flow": "^3.2.0",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.4.0",
    "faker": "^6.6.6",
    "fishery": "^2.2.2",
    "jest": "^27.5.1",
    "prettier": "^2.6.2",
    "supertest": "^6.0.0",
    "ts-jest": "^27.1.4",
    "ts-loader": "^9.2.8",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.6.3",
    "webpack": "^5.72.0"
  }
}
